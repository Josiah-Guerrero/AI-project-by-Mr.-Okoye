public static void sortDecreasing(String[] strs, int[] nums) {
		for (int i = 0; i < nums.length; i++) {
			int max = nums[i];
			int maxIndex = i;
			String temp;

			
			for (int j = i + 1; j < nums.length; j++) {
				if (nums[j] > max) {
					max = nums[j];
					maxIndex = j;
				}
			}
			
			if (maxIndex != i) {
			
				temp = strs[i];
				strs[i] = strs[maxIndex];
				strs[maxIndex] = temp;
    Scanner scan = new Scanner(System.in);
    boolean go = true;
    while(go){
      System.out.println("Input student first name or \"default\" for default values, \"q\" to quit:");
      String fn = scan.nextLine();
      if(fn.equals("default")){
        Student s = new Student();
        System.out.println("\n" + s + "\n");
      }
      else if(fn.equals("q")){
        go = false;
      }
      else{
        System.out.println("Input student last name:");
        String ln = scan.nextLine();
        System.out.println("Input year:");
        int y = scan.nextInt();
        System.out.println("Input GPA:");
        double g = scan.nextDouble();
        Student s = new Student(fn, ln, y, g);
        System.out.println("\n" + s + "\n");
        scan.nextLine();
      }
    }
  }
    }
